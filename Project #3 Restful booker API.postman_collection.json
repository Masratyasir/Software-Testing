				{
					"name": "Create Booking TC-Booking with future checkin dates",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Extracting the response body",
									"let responseBody = pm.response.json();",
									"",
									"// Parsing the checkin and checkout dates from the response",
									"let checkinDate = new Date(responseBody.bookingdates.checkin);",
									"let checkoutDate = new Date(responseBody.bookingdates.checkout);",
									"",
									"// Calculating the difference in months between the checkin and checkout dates",
									"let monthsDifference = (checkoutDate.getFullYear() - checkinDate.getFullYear()) * 12 + (checkoutDate.getMonth() - checkinDate.getMonth());",
									"",
									"// Checking if the booking is done for more than 6 months before",
									"if (monthsDifference > 6) {",
									"    pm.test(\"Booking done more than 6 months before\", function () {",
									"        pm.expect(monthsDifference).to.be.above(6);",
									"    });",
									"} else {",
									"    pm.test(\"Booking done less than 6 months before\", function () {",
									"        pm.expect(monthsDifference).to.be.below(6);",
									"    });",
									"}",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
